{% extends 'base.html.twig' %}

{% block title %}AdminMessages{% endblock %}

{% block body %}
    <h1>AdminMessages</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ admin_message.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ admin_message.name }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ admin_message.subject }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ admin_message.message }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ admin_message.status }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ admin_message.ip }}</td>
            </tr>
            <tr>
                <th>Note</th>
                <td>{{ admin_message.note }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ admin_message.createdAt ? admin_message.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ admin_message.updatedAt ? admin_message.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_messages_index') }}">back to list</a>

    <a href="{{ path('admin_messages_edit', {'id': admin_message.id}) }}">edit</a>

    {{ include('admin_messages/_delete_form.html.twig') }}
{% endblock %}
